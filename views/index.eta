<% layout('./template') %>

<div id="main-container" class="container">
  <div class="row">
      <div class="col-md-12">
        <h2><%= it.title %></h2>
      </div>
  </div>
  <div class="row">
      <div class="col-md-12">
        <div class="list-group">
        <% it.examples.forEach(function(example){ %>
          <a href="<%= example.href %>" class="list-group-item list-group-item-action"><%= example.text %></a>
        <% }) %>
        </div>
      </div>
  </div>
</div>
